<?xml version="1.0" encoding="US-ASCII"?>
<xml-bindings xmlns="http://www.eclipse.org/eclipselink/xsds/persistence/oxm"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.eclipse.org/eclipselink/xsds/persistence/oxm  http://www.eclipse.org/eclipselink/xsds/eclipselink_oxm_2_4.xsd"
	package-name="eu.agno3.runtime.messaging.xml"
	xml-mapping-metadata-complete="true" version="2.4">

	<xml-schema element-form-default="QUALIFIED"
		attribute-form-default="UNQUALIFIED" namespace="urn:agno3:message:1.0:base">
		<xml-ns prefix="m" namespace-uri="urn:agno3:message:1.0:base" />
	</xml-schema>

	<java-types>
		<java-type name="XmlMarshallableMessage" xml-accessor-type="NONE" super-type="java.lang.Object">

			<xml-root-element name="message" />

			<java-attributes>
			</java-attributes>
		</java-type>


		<java-type name="DefaultXmlErrorResponseMessage"
			xml-accessor-type="NONE">

			<xml-root-element name="error" />
		</java-type>

	</java-types>

</xml-bindings>
