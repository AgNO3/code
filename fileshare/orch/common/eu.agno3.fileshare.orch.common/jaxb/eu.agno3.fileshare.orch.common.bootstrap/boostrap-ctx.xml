<?xml version="1.0" encoding="US-ASCII"?>
<xml-bindings xmlns="http://www.eclipse.org/eclipselink/xsds/persistence/oxm"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.eclipse.org/eclipselink/xsds/persistence/oxm  http://www.eclipse.org/eclipselink/xsds/eclipselink_oxm_2_4.xsd"
	package-name="eu.agno3.fileshare.orch.common.bootstrap"
	xml-mapping-metadata-complete="true" version="2.4">

	<xml-schema element-form-default="QUALIFIED"
		namespace="urn:agno3:model:1.0:fileshare:bootstrap">
		<xml-ns prefix="fbc" namespace-uri="urn:agno3:model:1.0:fileshare:bootstrap" />
		<xml-ns prefix="bc" namespace-uri="urn:agno3:bootstrap:1.0" />
		<xml-ns prefix="hc" namespace-uri="urn:agno3:model:1.0:hostconfig" />
		<xml-ns prefix="fc" namespace-uri="urn:agno3:model:1.0:fileshare" />
		<xml-ns prefix="orchc" namespace-uri="urn:agno3:model:1.0:orchconfig" />
		<xml-ns prefix="cmb" namespace-uri="urn:agno3:model:1.0:base" />
		<xml-ns prefix="cmr" namespace-uri="urn:agno3:model:1.0:realm" />
	</xml-schema>

	<java-types>

		<java-type name="FileshareBootstrapContext"
			xml-accessor-type="NONE">
			<xml-type name="fileshareBootstrapContext" />

			<xml-root-element name="fileshareBootstrapContext" />

			<java-attributes>
				<xml-element name="fileshareService" java-attribute="fileshareService" />
				<xml-element name="fileshareConfig" java-attribute="fileshareConfig" />

				<xml-element name="fileshareCreateUser" java-attribute="createUser"
					required="false" />
				<xml-element name="fileshareCreateUserAdmin"
					java-attribute="createUserAdmin" required="false" />
				<xml-element name="fileshareCreateUserName"
					java-attribute="createUserName" required="false" />
				<xml-element name="fileshareCreateUserPassword"
					java-attribute="createUserPassword" required="false" />
			</java-attributes>
		</java-type>
	</java-types>

</xml-bindings>
